#!/usr/bin/env lunch
# this starts miville from the services branch ... using Lunch
# it starts it on local host and host brr.
# This is just an inspiration for you to change.
# see https://svn.sat.qc.ca/trac/postures/wiki/Lunch

add_command("~/src/miville/branches/services/py/mivilled", title="mivilled aalex", priority=1, sleep=1)
add_command("~/src/services/py/mivilled", user="miville", host="brrr", title="mivilled brrr", priority=1, sleep=1)
add_command("telnet localhost 14444", title="telnet aalex", priority=2)
add_command("telnet brrr 14444", title="telnet brrr", priority=2)


